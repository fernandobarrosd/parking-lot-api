// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model VehicleRented {
  licensePlate String @id
  type         String // Motocycle or Car
  rentedHours  Int
  rentedPrice  Float
}
